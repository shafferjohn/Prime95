	bump	rdi, 1*XMM_SCD2		;; Next sine/cosine pointer
	bump	rbx, 4*XMM_SCD2		;; Next sine/cosine pointer
	bump	rbp, 16*XMM_SCD2	;; Next sine/cosine pointer
