	r4_x4cl_four_complex_djbfft_preload
b7b:	r4_x4cl_four_complex_djbfft rsi, 64, 16*64, 32*64, rdi
	bump	rdi, 4*XMM_SCD2		;; Next sin/cos data
	loops	16, b7b			;; Test inner loop counter
