	bump	rsi, -4*32*64+dist128	;; Next source pointer
	loops	8, c4b			;; Loop 8 times
	bump	rcx, -128*64+8192+128	;; Next 8KB section to prefetch
	loops	2, c4a			;; Test loop counter
	bump	rsi, -2*8*dist128	;; Restore source pointer
