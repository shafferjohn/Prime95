	bump	rbx, -2*4*XMM_SCD2	;; Restore sine/cosine pointer
	bump	rbp, -2*16*XMM_SCD2	;; Restore sine/cosine pointer
	loops	5, c5b			;; Loop 5 times
	bump	rsi, -5*dist128		;; Restore source pointer
