c1a:	xtouch	[rcx+4096-64]		;; Load prefetch TLBs
	xtouch	[rcx+2*4096-64]
c1b:	r5_h5cl_ten_reals_five_complex_djbunfft rsi, 64, 2*dist128, rdi, 256*XMM_SCD2, rdx, 256*XMM_HSCD2
