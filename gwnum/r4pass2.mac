	bump	rsi, -2*64+8*64		;; Next source pointer
	bump	rdi, -2*16*XMM_SCD2	;; Restore sin/cos ptr
	loops	16, c9b			;; Test loop counter
	bump	rsi, -16*8*64+dist128	;; Next source pointer
