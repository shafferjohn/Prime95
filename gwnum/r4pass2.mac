	loops_init 512
	r4_h4cl_eight_reals_four_complex_djbfft_preload
b8b:	r4_h4cl_eight_reals_four_complex_djbfft rsi, 64, 4*64, 8*64, rdi, rdx
	bump	rdi, 16*XMM_SCD2	;; Next sin/cos data
