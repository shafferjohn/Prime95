	r3_h6r_h3c_djbfft_mem [srcreg+d1][rbx],[srcreg+16][rbx],[srcreg+2*d1+16][rbx],[srcreg+d1+32][rbx],[srcreg+48][rbx],[srcreg+2*d1+48][rbx],screg1+scoff1,screg2+scoff2,srcreg+srcinc,d1
