	mulpd	r2, xmm15		;; 0.866 * (R2 - R3)
	subpd	r1, t1			;; (R1-.5R2-.5R3)
	mulpd	r5, xmm15		;; 0.866 * (I2 - I3)
	xload	r4, memi1
	addpd	r6, r4			;; I1 + I2 + I3 (final I1)
	subpd	r4, t2			;; (I1-.5I2-.5I3)

