	yloop	2, c3b, -2*dist64+64	;; Test loop counter, next source pointer
	yloop	8, c3b, -8*64+2*dist64, rdi, -32*YMM_SCD3 ;; Test loop counter, next source pointer, restore sine/cosine pointer
