	yloop	16, b4b, -16*64+4*dist64, rdi, -64*YMM_SCD3 ;; Test loop counter, next source pointer, restore sin/cos pointer
	yloop	25, b4b, -25*4*dist64	;; Test loop counter, restore source pointer

