	yloop	4, b5a, -4*dist64+64	;; Test loop counter, next source pointer
	yloop	4, b5a, -4*64+16*64, rdi, -16*4*YMM_SCD3 ;; Test loop counter, next source pointer, restore sin/cos pointer

