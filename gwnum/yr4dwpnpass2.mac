	yloop	64, b3b, -64*64+4*dist64, rbp, -64*YMM_SCD2 ;; Test loop counter, next source pointer, restore sin/cos ptr
	yloop	25, b3b, -25*4*dist64	;; Loop 25 times, restore source pointer

