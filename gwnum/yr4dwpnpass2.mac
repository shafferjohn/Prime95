	yloop	4, c3a, -4*4*dist64+64	;; Test loop counter, next source pointer
	yloop	64, c3a, -64*64+16*dist64, rdi, -256*YMM_SCD3 ;; Test loop counter, next source pointer, restore sine/cosine pointer

