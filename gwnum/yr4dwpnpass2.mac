	jg	ypass2_6_levels_complex_3

ypass2_6_levels_complex_2:
	start_timer 9
	yloop_init 4
	yloop_set_prefetch YSINGLE
	yr4_4cl_four_complex_with_square_preload
b3b:	yr4_4cl_four_complex_with_square rsi, 64, 4*64, 8*64, 4
