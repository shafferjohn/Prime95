	ystore	[srcreg+32], ymm4		;; Save I1
	ystore	[srcreg+d1], ymm5		;; Save R2
	ystore	[srcreg+d1+32], ymm7		;; Save I2
	ystore	[srcreg+d2], ymm2		;; Save R3
	ystore	[srcreg+d2+32], ymm3		;; Save I3
	ystore	[srcreg+d2+d1], ymm1		;; Save R4
