	ystore	[srcreg+d2], ymm5	;; Save R3
	ystore	[srcreg+d2+32], ymm4	;; Save R7/I3
	ystore	[srcreg+d2+d1], ymm7	;; Save R4
	ystore	[srcreg+d2+d1+32], ymm3	;; Save R8/I4
