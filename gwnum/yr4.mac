					;; Eight reals comments		; Four complex comments
					;; new R3 = A2
					;; new R4 = B2
					;; new R5 = A3
					;; new R6 = B3
					;; new R7 = A4
					;; new R8 = B4

	vsubpd	ymm2, ymm8, ymm5	;; R6 - R8 (new R8)		; I3 - I4 (new R4)
