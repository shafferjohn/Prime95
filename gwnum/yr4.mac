	ENDM
yr4_b4cl_csc_wpn4_eight_reals_fft MACRO srcreg,srcinc,d1,d2,screg1,scinc1,screg2,scinc2,maxrpt,L1pt,L1pd
	vmovapd	ymm0, [srcreg+d2+d1]	;; R4
