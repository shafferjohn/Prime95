	vsubpd	ymm4, ymm7, ymm1	;; R5 - R7 (new R6)		; R3 - R4 (new negI4)
	vaddpd	ymm7, ymm7, ymm1	;; R5 + R7 (new R5)		; R3 + R4 (new R3)

	vmovapd	ymm1, mem2		;; R2				; I1
