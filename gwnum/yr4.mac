	ystore	[srcreg+d1], ymm2	;; Save R2
	ystore	[srcreg+d1+32], ymm7	;; Save I2
	ystore	[srcreg+d2], ymm5	;; Save R3
	ystore	[srcreg+d2+32], ymm6	;; Save I3
	ystore	[srcreg+d2+d1], ymm3	;; Save R4
	ystore	[srcreg+d2+d1+32], ymm4	;; Save I4
	bump	srcreg, srcinc
	bump	screg1, scinc1
	bump	screg2, scinc2
	ENDM


