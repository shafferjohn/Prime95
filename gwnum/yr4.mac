yr4_s4cl_csc_eight_reals_fft_preload MACRO
	yr4_s8r_fft_cmn_preload
	ENDM
yr4_s4cl_csc_eight_reals_fft MACRO srcreg,srcinc,d1,d2,screg,scinc,maxrpt,L1pt,L1pd
