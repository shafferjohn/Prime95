	vmulpd	ymm2, ymm2, ymm6		;; C3 = I3 * sine
	vmulpd	ymm4, ymm4, ymm6		;; D3 = R3 * sine

	vaddpd	ymm1, ymm1, ymm2		;; A3 = A3 + C3 (new R3)
	vsubpd	ymm0, ymm0, ymm4		;; B3 = B3 - D3 (new I3)

	vmovapd	ymm2, [screg+192+32]		;; cosine
