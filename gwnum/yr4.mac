	vmulpd	ymm3, ymm3, [screg+64]	;; B2 = B2 * sine (new I2)		; 10-14

	vaddpd	ymm1, ymm1, ymm4	;; A4 = A4 + I4				; 10-12
	vsubpd	ymm5, ymm5, ymm2	;; B4 = B4 - R4				; 11-13

	vmovapd	ymm4, [screg]		;; Sine
