	yp_complex_mult ymm7, ymm3, [srcreg+d2][rbp], [srcreg+d2+32][rbp], ymm4, ymm5 ;; Mult R3, I3
	yp_complex_mult ymm0, ymm6, [srcreg+d2+d1][rbp], [srcreg+d2+d1+32][rbp], ymm4, ymm5 ;; Mult R4, I4
	ystore	[srcreg+d1], ymm2		;; Save R2
