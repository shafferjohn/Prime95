	bump	rsi, -2*4*clmblkdst-3*clmblkdst8;; Restore source pointer
b4b:	x2cl_eight_reals_fft_1 rsi, 64, 4*clmblkdst
	add	al, 256/2		;; Test inner loop counter
	jnc	b4b			;; Iterate if necessary

