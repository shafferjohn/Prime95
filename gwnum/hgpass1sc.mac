	sub	al, 1			;; Test loop counter
	jnz	c3a			;; Iterate if necessary
	bump	rsi, -8*clmblkdst+2*clmblkdst8;; Next source pointer
	bump	rdi, XMM_SCD		;; Next sine/cosine pointer
