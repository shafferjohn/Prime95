	jnc	b1b
	bump	rsi, -clm*2*64+clmblkdst;; Next source pointer
	bump	rcx, -clm*128+7*blkdst	;; Next prefetch pointer
	ELSE
	bump	rsi, -7*clmblkdst8+clmblkdst;; Next source pointer
	ENDIF

