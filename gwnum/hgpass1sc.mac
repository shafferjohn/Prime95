	start_timer 26
	mov	rdi, sincos1		;; Load sin/cos pointer
b8b:	g4cl_eight_reals_fft_2 rsi, 64, clmblkdst, 2*clmblkdst, rcx, 64, blkdst, 2*blkdst
	add	al, 256/2/clm		;; Test inner loop counter
	jnc	b8b			;; Iterate if necessary

