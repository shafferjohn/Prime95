	bump	rcx, -2*clm*64+4*blkdst	;; Next dest pointer
	bump	rdi, 2*XMM_SCD		;; Next sine/cosine pointer
	sub	al, 1
	jnz	b9b
	bump	rsi, -8*clmblkdst+clmblkdst8
	sub	ah, 1			;; Test outer loop counter
	mov	al, 2
	jnz	b9b			;; Iterate if necessary
	end_timer 26

