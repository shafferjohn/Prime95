	add	ah, 256/2
	jnc	b2b

;;	bump	rsi, -2*2*clmblkdst8+4*clmblkdst8;; Next source pointer
b3b:	x4cl_four_complex_fft rsi, 64, 4*clmblkdst8, 8*clmblkdst8, rdi
	xloop1sp b3b, 2*clmblkdst8
