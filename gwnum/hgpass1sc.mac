	mov	eax, 63*65536+2		;; 63 iterations of 2 iters of 2*2*clm
b7a:	xtouch	[rcx+(clm-1)*128]	;; Preload TLB
b7b:	x4cl_four_complex_fft rsi, 64, 4*clmblkdst, clmblkdst8, rdi
	xloop2	b7b, clmblkdst
