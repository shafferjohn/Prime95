yr4dwpn_pass1sc256 MACRO fftname, pass2_levels, clmarg
	LOCAL	b0b, b1b, b6b, b7b, b8b, b9a, b9aa, b9b, c0b, c1b, c6b, c7b, c8b, c9a, c9aa, c9b
