	yloop	4, b9b, 0, rdi, clm*YMM_SCD4	;; Test loop counter, next premult/sine/cosine pointer
	yloop	2, b9b, -8*clmblkdst+clmblkdst8 ;; Test loop counter, next source pointer
	yloop	2, b9b				;; Test loop counter
	end_timer 26

