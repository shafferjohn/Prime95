c7b:	yr4_b4cl_wpn4_four_complex_djbunfft rsi, 64, clmblkdst, 2*clmblkdst, rdx, 32, rbx, YMM_SCD4, 4*clm, L1PREFETCH_ALL, r9 ;; Prefetch next scratch area section
