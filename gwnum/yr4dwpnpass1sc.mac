	lea	rbx, [rdx+clm*YMM_SCD2]		;; Real data sin/cos pointer, will become delayed sin/cos multipliers
	lea	rdx, YMM_TMPS			;; Computed clm*SCD4 sin/cos values
	yloop_set_prefetch YNONE		;; Prefetch no cache lines
