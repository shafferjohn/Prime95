	bump	rdi, 2*4*clm*16			;; Sin/cos ptr
	lea	rdx, [rdi-2*4*clm*8]		;; Sin/cos ptr
	yloop_init 24*4*clm			;; 24*4*clm iterations
	yloop_set_clm_prefetch YSINGLE
