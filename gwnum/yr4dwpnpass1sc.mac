	mov	rsi, scratch_area		;; Get address of scratch area
	set_data_prefetch_ptrs			;; Init prefetching pointers
	yloop_init 4*4*clm			;; 4*4*clm iterations
	yloop_set_prefetch YNONE		;; Prefetch no cache lines
