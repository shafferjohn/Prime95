	yloop_set_clm_prefetch YNONE
	yr4_4cl_csc_eight_reals_unfft_preload
c4b:	yr4_4cl_csc_eight_reals_unfft rsi, 64, 4*clmblkdst, clmblkdst8, rdi, 0, 4, L1PREFETCH_ALL, 2*clmblkdst8 ;; Prefetch short dist ahead
