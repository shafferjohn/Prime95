b8b:	yr8_rsc_sg8cl_2sc_sixteen_reals_fft8 rsi, 256, 64, 128, clmblkdst, rbp, 64, blkdst, 2*blkdst, 4*blkdst, rdx, YMM_SCD8, rbx, YMM_SCD4, clm, L1PREFETCH_DEST_ALL, 8*blkdst
