	yloop	clm, c9b, -clm*256+2*clmblkdst, rbp, -clm*64+8*blkdstreg, r13, -clm*64+8*blkdstreg, rdx, -clm*YMM_SCD8, r12, -clm*64+8*blkdstreg, r14, -clm*64+8*blkdstreg ;; Loop, next src/dest ptrs, restore sin/cos ptr
