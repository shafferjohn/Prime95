	yloop	4*clm, b7b, -4*clm*64+clmblkdst	;; 4*clm iterations
	yloop	2, b7b, -2*clmblkdst+clmblkdst8, rdx, -2*4*clm*8, rdi, -2*4*clm*YMM_SCND4 ;; Loop, restore sin/cos ptrs
