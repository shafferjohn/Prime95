	mulpd	xmm2, [screg+0]			;; B1 = B1 * sine (new I1)
	mulpd	xmm4, [screg+128]		;; B2 = B2 * sine (new I2)
	mulpd	xmm6, [screg+64]		;; B3 = B3 * sine (new I3)
	mulpd	xmm5, [screg+192]		;; B4 = B4 * sine (new I4)

