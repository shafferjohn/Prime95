	xcopy	xmm5, xmm7			;; Copy R5
	subpd	xmm7, xmm3			;; R5 = R5 - R6 (new R6)
	addpd	xmm3, xmm5			;; R6 = R5 + R6 (new R5)

	xload	xmm5, [srcreg+d2+d1+48]		;; I8
