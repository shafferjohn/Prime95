	addpd	xmm1, xmm5	 		;; A4 = A4 + I4
	mulpd	xmm5, [screg+192+16]		;; B4 = I4 * cosine/sine

	subpd	xmm2, [srcreg]			;; B1 = B1 - R1
	subpd	xmm4, [srcreg+32]		;; B2 = B2 - R2
	subpd	xmm6, [srcreg+d1]		;; B3 = B3 - R3
	mulpd	xmm0, [screg+64]		;; A3 = A3 * sine (new R3)
