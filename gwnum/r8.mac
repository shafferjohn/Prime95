IFDEF UNUSED
r8_g4cl_eight_complex_unfft8 MACRO srcreg,srcinc,d1,d2,dstreg,dstinc,e1,e2,screg
	xload	xmm7, [screg+0+16]		;; cosine/sine
	mulpd	xmm7, [srcreg]			;; A1 = R1 * cosine/sine
	xload	xmm3, [screg+128+16]		;; cosine/sine
